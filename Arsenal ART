",false)local c=a:CreateToggle({Name="Box",CurrentValue=false,Flag="Box",Callback=function(a)if a then b.teamSettings.enemy.box=true else b.teamSettings.enemy.box=false end end})local c=a:CreateToggle({Name="Name",CurrentValue=false,Flag="Name",Callback=function(a)if a then b.teamSettings.enemy.name=true else b.teamSettings.enemy.name=false end end})local c=a:CreateToggle({Name="Distance",CurrentValue=false,Flag="Distance",Callback=function(a)if a then b.teamSettings.enemy.distance=true else b.teamSettings.enemy.distance=false end end})local c=a:CreateToggle({Name="Tracers",CurrentValue=false,Flag="Tracers",Callback=function(a)if a then b.teamSettings.enemy.tracer=true else b.teamSettings.enemy.tracer=false end end})local c=a:CreateButton({Name="Active ESP",Interact='Click',Callback=function()b.Load()end})local a=a:CreateButton({Name="Desactive ESP",Interact='Click',Callback=function()b.Unload()end})
